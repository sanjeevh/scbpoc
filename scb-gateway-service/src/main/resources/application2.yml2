server:
  port: 8080
spring:
  application:
    name: gatewayservice
eureka: 
  instance : 
    preferIpAddress: true
      
  client: 
    registerWithEureka: true 
    fetchRegistry: true 
    registryFetchIntervalSeconds: 5  
    serviceUrl:
      defaultZone: http://localhost:8761/eureka/


ribbon:  
  eureka:
    enabled: true
  ConnectTimeout: 3000 
  ReadTimeout : 60000
  SocketTimeout: 60000 
  


    

 
       

      